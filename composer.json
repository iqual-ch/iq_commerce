{
  "name": "iqual/iq_commerce",
  "description": "Provides additional functionality for the Drupal Commerce",
  "type": "drupal-custom-module",
  "repositories": [
    {
      "type": "composer",
      "url": "https://github.com/iqual-ch/iq_commerce"
    }
  ],
  "require": {
    "php": "^8.1",
    "drupal/core": "^9.5 || ^10",
    "drupal/commerce": "^2.0",
    "drupal/hierarchical_taxonomy_menu": "^2.0",
    "drupal/media_entity_browser": "^2.0",
    "drupal/commerce_variation_add_to_cart": "^2.0",
    "drupal/commerce_variation_cart_form": "^1.0",
    "drupal/commerce_shipping": "^2.0",
    "drupal/pagedesigner_block_adaptable": "^3.1",
    "iqual/iq_progressive_decoupler": "^3.0",
    "drupal/term_reference_tree": "^2.0",
    "drupal/twig_tweak": "^3.0",
    "drupal/symfony_mailer": "^1.4",
    "drupal/commerce_cart_api": "^1.7",
    "iqual/iq_entity_browsers": "^3.0",
    "drupal/commerce_combine_carts": "^1.0",
    "drupal/login_emailusername": "^2.0",
    "drupal/redirect_after_login": "^3.0",
    "drupal/single_datetime": "^2.0",
    "drupal/entity_print": "^2.0",
    "drupal/commerce_repeat_order": "^2.2"
  },
  "extra": {
    "patches": {
      "drupal/commerce_repeat_order": {
        "[https://dgo.to/3367273]: Drupal 10 compatibility": "https://www.drupal.org/files/issues/2023-09-13/d10_compatibility-3367273-7.patch"
      }
    }
  }
}
